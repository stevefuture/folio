{
  "name": "photographer-portfolio",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "next build && next export",
    "postbuild": "npm run generate-seo && npm run generate-sitemap",
    "generate-seo": "node scripts/generate-seo.js",
    "generate-sitemap": "node scripts/generate-sitemap.js",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "deploy:s3:staging": "npm run export && aws s3 sync out/ s3://staging-bucket-name/ --delete",
    "deploy:s3:production": "npm run export && aws s3 sync out/ s3://production-bucket-name/ --delete",
    "invalidate:staging": "aws cloudfront create-invalidation --distribution-id STAGING_DISTRIBUTION_ID --paths '/*'",
    "invalidate:production": "aws cloudfront create-invalidation --distribution-id PRODUCTION_DISTRIBUTION_ID --paths '/*'",
    "deploy:staging": "npm run deploy:s3:staging && npm run invalidate:staging",
    "deploy:production": "npm run deploy:s3:production && npm run invalidate:production"
  },
  "dependencies": {
    "next": "^14.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "@aws-amplify/ui-react": "^6.0.0",
    "aws-amplify": "^6.0.0",
    "framer-motion": "^10.0.0",
    "react-swipeable": "^7.0.1",
    "sharp": "^0.32.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "typescript": "^5.0.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0"
  }
}
