{
  "environment": "production",
  "description": "Production environment for live website",
  "config": {
    "priceClass": "PriceClass_All",
    "cacheTtl": {
      "html": 3600,
      "images": 2592000,
      "assets": 31536000
    },
    "monitoring": {
      "enableDetailedMetrics": true,
      "billingAlarmThreshold": 100
    },
    "security": {
      "enableWaf": true,
      "rateLimitPerMinute": 2000
    }
  }
}
